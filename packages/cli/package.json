{
  "name": "@ereo/cli",
  "version": "0.1.33",
  "license": "MIT",
  "author": "Ereo Team",
  "homepage": "https://ereo.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/ereojs/ereo.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/ereojs/ereo/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ereo": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --external @ereo/core --external @ereo/router --external @ereo/server --external @ereo/bundler && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "bun run ./src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ereo/core": "workspace:*",
    "@ereo/router": "workspace:*",
    "@ereo/server": "workspace:*",
    "@ereo/trace": "workspace:*"
  },
  "devDependencies": {
    "@ereo/bundler": "workspace:*",
    "@types/bun": "^1.1.0",
    "typescript": "^5.4.0"
  }
}
